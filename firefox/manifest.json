{
  "manifest_version": 2,
  "name": "AcceptAllCookies",
  "description": "Accepts all cookies on web pages.",
  "version": "0.0.1",
  "icons": {
    "16": "/icons/cookies-16.png",
    "48": "/icons/cookies-48.png",
    "128": "/icons/cookies-128.png"
  },
  "permissions": [],
  "browser_specific_settings": {
    "gecko": {
      "id": "acceptallcookies@example.com"
    }
  },
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["app.js"]
  }]
}